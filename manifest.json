{
  "manifest_version": 3,
  "name": "ProcrastiNate",
  "description": "Stay on top of your assignments with iCal integration.",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://canvas.tamu.edu/*"],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "192": "icons/icon192.png"
      }
  },
  "content_scripts": [],
  "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "192": "icons/icon192.png"
  }
}
